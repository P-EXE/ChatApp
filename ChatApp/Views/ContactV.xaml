<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:m="clr-namespace:ChatShared.Models;assembly=ChatShared"
             x:Class="ChatApp.Views.ContactV">
  <VerticalStackLayout x:DataType="m:AppUser" Spacing="4" Margin="0,10">
    <Label Text="Display Name" FontSize="18"/>
    <Label Text="{Binding UserName}" FontSize="12"/>
  </VerticalStackLayout>
</ContentView>
